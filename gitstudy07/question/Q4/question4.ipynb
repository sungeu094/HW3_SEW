   "source": [
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)\n",
    "def main():\n",
    "    for i in range(0, 15):\n",
    "        if i % 2 == 0:\n",
    "            print(f\"{i}! = {factorial(i)}\")\n",
    "            \n",
    "if __name__ == '__main__':\n",
    "    main()\n"
   ]
  }
